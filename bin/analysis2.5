#!/usr/bin/env bash

SCRIPTDIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" || exit && pwd)"
source "${SCRIPTDIR}/paths.sh"

ANALYSIS="${CCPNMR_TOP_DIR}/${VERSIONPATH}"/python/ccpnmr/analysis/AnalysisGui.py
if [[ ${UNAME} == *"Win"* || ${UNAME} == *"MING"* ]]; then
    echo "Please use the scripts in the 'bat' directory"
else
    "${ANACONDA3}"/bin/python -i -O -W ignore::DeprecationWarning "${ANALYSIS}" $*
fi
